{
    "openapi": "3.0.0",
    "info": {
        "title": "Scootin",
        "description": "Scootin REST API specifications.",
        "version": "1.0"
    },
    "paths": {
        "/api/scooter/{id}/history": {
            "post": {
                "description": "Insert new history events for a particular scooter.",
                "operationId": "19c1d4f517ab5a5219ed26cca0d3c87c",
                "parameters": [
                    {
                        "name": "id",
                        "in": "path",
                        "description": "The id of the scooter.",
                        "required": true
                    }
                ],
                "requestBody": {
                    "content": {
                        "application/json": {
                            "schema": {
                                "properties": {
                                    "latitude": {
                                        "type": "number",
                                        "format": "float"
                                    },
                                    "longitude": {
                                        "type": "number",
                                        "format": "float"
                                    },
                                    "userId": {
                                        "type": "integer"
                                    },
                                    "status": {
                                        "type": "integer"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "OK"
                    },
                    "401": {
                        "description": "Not allowed"
                    },
                    "500": {
                        "description": "Internal error"
                    }
                }
            }
        },
        "/api/scooters/nearby": {
            "get": {
                "description": "Find scooters that are nearby a particular geographical position (up to 1 km).",
                "operationId": "c1b56d152be12205e8755bb6d249247d",
                "parameters": [
                    {
                        "name": "latitude",
                        "in": null,
                        "required": true
                    },
                    {
                        "name": "longitude",
                        "in": null,
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
                        "description": "List of scooters nearby.",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "result": {
                                            "type": "array",
                                            "items": {
                                                "properties": {
                                                    "scooter_id": {
                                                        "type": "integer"
                                                    },
                                                    "distance": {
                                                        "description": "Distance in KM",
                                                        "type": "number",
                                                        "format": "float"
                                                    },
                                                    "latitude": {
                                                        "type": "number",
                                                        "format": "float"
                                                    },
                                                    "longitude": {
                                                        "type": "number",
                                                        "format": "float"
                                                    }
                                                },
                                                "type": "object"
                                            }
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    },
                    "400": {
                        "description": "Request missing parameters"
                    }
                }
            }
        }
    }
}